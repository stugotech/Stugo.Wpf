<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <!-- redefine this property in your project file to change the import -->
    <TargetFrameworkMoniker Condition="'$(TargetFrameworkMoniker)' == ''">net40</TargetFrameworkMoniker>
  </PropertyGroup>
  <ItemGroup>
    <WixLibrary Include="Stugo.Wpf.$(TargetFrameworkMoniker)">
      <HintPath>$(MSBuildThisFileDirectory)\Stugo.Wpf.$(TargetFrameworkMoniker).wixlib</HintPath>
      <Name>Stugo.Wpf</Name>
    </WixLibrary>
  </ItemGroup>
</Project>
